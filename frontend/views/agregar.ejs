<body>
    <%- include('partials/nav.ejs') %>
    <div class="contenedor">
        <form class="agregar_tarea_form" id="agregar_form" action="">
            <h2>Agregar Tarea</h2>
            <input type="text" name="" id="" placeholder="Nombre de la tarea">
            <textarea name="" id="" rows="15" placeholder="Descripcion de la tarea"></textarea>
            <input type="date" placeholder="fecha estimada">
            <input type="text" placeholder="fecha inicio">
            <button type="submit">Agregar Tarea</button>
        </form>
    </div>


    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
